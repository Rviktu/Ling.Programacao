include <stdio.h>

int principal() {
    char nome[50];
    flutuar nota1, nota2, mídia;

    printf("Digite o nome do aluno: ");
    scanf("%s", nome);

    printf("Digite a nota 1: ");
    scanf("%f", ¬a1);

    printf("Digite a nota 2: ");
    scanf("%f", ¬a2);

    mídia = (nota1 + nota2) / 2;

    printf("\n");
    printf("Nome: %s\n", nome);
    printf("Nota 1: %.2f\n", nota1);
    printf("Nota 2: %.2f\n", nota2);
    printf("Média: %.2f\n", media);

    if (mídia >= 6) {
        printf("Aprovado.\n");
    } outro {
        printf("Reprovado.\n");
    }

    retorna 0;
}
