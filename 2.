#include <stdio.h>

int principal() {
    int num1, num2, soma;

    printf("Digite o primeiro número: ");
    scanf("%d", &num1);

    printf("Digite o segundo número: ");
    scanf("%d", &num2);

    soma = num1 + num2;

    if (soma > 20) {
        soma += 8;
    } outro {
        soma -= 5;
    }

    printf("Resultado: %d\n", soma);

    retorna 0;
}
